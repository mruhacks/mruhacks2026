{
  "name": "mruhacks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "db:start": "echo 'Starting database…' && docker compose up -d && echo 'Database started.'",
    "db:stop": "echo 'Stopping database…' && docker compose down && echo 'Database stopped.'",
    "db:reset": "echo 'Resetting database (dropping volumes)…' &&        docker compose down -v &&        echo 'Starting fresh database…' &&        docker compose up -d &&        pnpm db:wait &&        echo 'Running migrations…' &&        drizzle-kit migrate && tsx scripts/seed.ts &&        echo 'Database reset complete.'",
    "db:seed": "echo 'Seeding Database' && tsx scripts/seeds.ts",
    "db:wait": "echo 'Waiting for Postgres to be ready…' &&        until docker exec mruhacks-db-dev pg_isready >/dev/null 2>&1; do          printf '.';          sleep 1;        done &&        echo ' Postgres is ready.'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "better-auth": "^1.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.546.0",
    "next": "16.1.4",
    "next-themes": "^0.4.6",
    "pg": "^8.17.2",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-select": "^5.10.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@databases/pg-test": "^3.1.2",
    "@eslint/eslintrc": "^3.3.3",
    "@faker-js/faker": "^10.2.0",
    "@next/eslint-plugin-next": "^15.5.9",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.30",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.5.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "install": "^0.13.0",
    "npm": "^11.8.0",
    "tailwindcss": "^4.1.18",
    "testcontainers": "^11.11.0",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "react-hook-form": "^7.65.0",
      "@hookform/resolvers": "^5.2.2"
    }
  }
}
